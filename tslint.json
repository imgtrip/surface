{
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "semicolon": [
      false,
      "always"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "trailing-comma": [
      false
    ]
  }
}
